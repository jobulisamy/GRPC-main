{
    "compilerOptions": {
      "target": "ESNext", // or ESNext
      "module": "ESNext", // or "CommonJS"
      "moduleResolution": "Node",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "src/proto/**/*.proto"],
    "exclude": ["node_modules"]
  }
  